#include <stdio.h>
#include<stdlib.h>


int main() {
    
    int age = 30;
    double gpa = 3.6;
    char grade = 'A';
    printf("age: %p\n gpa: %p\ngrade: %p", &age, &gpa, &grade);
    
    return 0;
}
