#include <stdio.h>
#include<stdlib.h>

int main() {
    int age;
    double gpa;
    
    char name[20];
    printf("Enter your age");
    scanf("%d",&age);
    printf("Your age is %d years",age);
    printf("Enter your gpa");
    scanf("%lf",&gpa);
    printf("Your gpa is %f", gpa);
   
    printf("Enter your name");
    scanf("%s",name);
    printf("Your name is %s", name);
    return 0;
}
