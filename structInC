#include <stdio.h>
#include<stdlib.h>

struct Student{
    char name[50];
    char major[50];
    int age;
    double gpa;
};
int main() {
    struct Student student1;
    student1.age=22;
    student1.gpa= 3.5;
    strcpy(student1.name,"Jack");
    strcpy(student1.major, "ComputerScience");
    
    struct Student student2;
    student2.age=26;
    student2.gpa= 2.5;
    strcpy(student2.name,"Era");
    strcpy(student2.major, "Business");
    printf("%s",student2.name);
    return 0;
}
