#include <stdio.h>
#include<stdlib.h>

int main() {
    double num1;
    double num2;
    char op;
    
    printf("Enter a number: ");
    scanf("%lf", &num1);
    printf("Enter another number:");
    scanf("%lf", &num2);
    printf("Enter an operator");
    scanf(" %c", &op);
    
    if(op=='+'){
        printf("%f",num1+num2);
    } else if(op== '-'){
        printf("%f",num1-num2);
    } else if(op == '*'){
        printf("%f",num1*num2);
    } else if (op == '/'){
        printf("%f",num1/num2);
    }else {
        printf("Invalid Operator");
    }
    return 0;
}
